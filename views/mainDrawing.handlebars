
    <script>
        var decodeJson=decodeURIComponent("{{{encodedJSON}}}");
        var arr=decodeJson.split(";");
        OBJECT_ARRAY=[];
        for(let i=0; i<arr.length; i++){
            let arr2=arr[i].split(',');
            let name=arr2[0];
            let x=parseFloat(arr2[1]);
            let y=parseFloat(arr2[2]);
            let l=parseFloat(arr2[3]);
            let w=parseFloat(arr2[4]);
            let h=parseFloat(arr2[5]);
            let re=parseInt(arr2[6]);
            let gr=parseInt(arr2[7]);
            let bl=parseInt(arr2[8]);
            let obj=new DrawObj(name, x, y, l, w, h, re, gr, bl);
            OBJECT_ARRAY.push(obj);
        }
    </script>







